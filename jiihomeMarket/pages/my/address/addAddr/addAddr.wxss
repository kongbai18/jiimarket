#box{
  background: #f3f3f3;
  min-height: 1140rpx;
  overflow: hidden;
}
#info-box{
  margin-top:5rpx;
  padding: 0 32rpx;
  background: white;
}
.info{
  height: 100rpx;
  border-bottom: 1rpx solid #f3f3f3;
  overflow: hidden;
}
.info-name{
  float: left;
  height: 100rpx;
  width:150rpx;
  font-size: 30rpx;
  line-height: 100rpx;
}
.info-text{
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;

}
.default{
  height: 80rpx;
  width: 750rpx;
  margin-top: 20rpx;
  background: white;
  overflow: hidden;
}
.default-cho{
  float: left;
  margin: 25rpx 20rpx;
  width: 30rpx;
  height: 30rpx;
  border-radius: 15rpx;
  border: 1rpx solid #f3f3f3;
}
.default-text{
  float:left;
  height: 80rpx;
  width: 180rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: gray;
}
#bottom{
  position: fixed;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background: #ffc700;
  font-size: 38rpx;
  border-top: 1rpx solid #f3f3f3;
  bottom: 0;
}